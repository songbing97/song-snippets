{
  "v-for": {
    "prefix": "vfor",
    "body": [
        "v-for=\"item in ${2:items}\" :key=\"item\""
    ],
    "description": "v-for"
  },
  "v-for & index": {
    "prefix": "vfori",
    "body": [
      "v-for=\"(item, index) in ${2:items}\" :key=\"item\""
    ],
    "description": "v-for & index"
  }
}