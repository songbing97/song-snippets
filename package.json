{
  "name": "song-snippets",
  "displayName": "Song Snippets",
  "description": "",
  "version": "0.0.1",
  "icon": "img/logo.png",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "snippet",
    "javascript",
    "vue",
    "react"
],
  "activationEvents": [],
