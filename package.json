{
  "name": "song-snippets",
  "displayName": "Song Snippets",
  "description": "",
  "version": "0.0.15",
  "icon": "img/logo.png",
  "publisher": "songbing",
  "engines": {
    "vscode": "^1.80.0"
  },
  "categories": [
    "Snippets"
  ],
  "keywords": [
    "snippet",
    "javascript",
    "vue",
    "react",
    "typescript"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/songbing97/song-snippets"
  },
  "homepage": "https://github.com/songbing97/song-snippets/README.md",
  "activationEvents": [],
  "main": "./extension.js",
  "contributes": {
    "languages": [
      {
        "id": "vue",
        "aliases": [
          "Vue",
          "vue"
        ],
        "extensions": [
          ".vue"
        ]
      }
    ],
    "snippets": [
      {
        "language": "javascript",
        "path": "./snippets/js.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/react.json"
      },
      {
        "language": "javascriptreact",
        "path": "./snippets/js.json"
      },
      {
        "language": "html",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue-html",
        "path": "./snippets/html.json"
      },
      {
        "language": "html",
        "path": "./snippets/vue.json"
      },
      {
        "language": "vue",
        "path": "./snippets/js.json"
      },
      {
        "language": "vue",
        "path": "./snippets/html.json"
      },
      {
        "language": "vue",
        "path": "./snippets/vue.json"
      },
      {
        "language": "typescript",
        "path": "./snippets/ts.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/ts.json"
      },
      {
        "language": "typescriptreact",
        "path": "./snippets/tsx.json"
      }
    ]
  },
  "scripts": {
    "build": "vsce package",
    "update": "vsce publish patch",
    "lint": "eslint .",
    "pretest": "npm run lint",
    "test": "node ./test/runTest.js"
  },
  "devDependencies": {
    "@types/vscode": "^1.80.0",
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3",
    "@vscode/test-electron": "^2.3.2"
  }
}
